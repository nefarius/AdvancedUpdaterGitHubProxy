{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
      "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
      "WriteTo": [
          {
              "Name": "File",
              "Args": {
                  "path": "logs/server-.log",
                  "rollingInterval": "Day"
              }
          },
          {
              "Name": "Console"
          }
      ]
  },
  "AllowedHosts": "*"
}
